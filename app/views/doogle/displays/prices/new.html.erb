<%= render 'doogle/displays/submenu' %>

<% title "New Pricing for #{@price.display.model_number}" %>

<%= semantic_form_for @price, :url => doogle_display_prices_url(@display), :html => {:class => 'doogle'} do |f| %>
  <%= render :partial=>'form', :locals =>{:f=>f} %>
  <%= f.buttons do %>
    <%= f.commit_button 'Create' %>
	  <%= f.cancel_link %>
  <% end %>
<% end %>